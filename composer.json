{
    "name": "edvakf/php-thrift-mapper",
    "description": "Convert a PHP array into an Apache Thrift struct type.",
    "require": {
        "apache/thrift": "0.9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "ext-xdebug": "Allows code coverage reporting (pecl install xdebug)"
    },
    "autoload": {
        "psr-4": {
            "ThriftMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThriftMapper\\Test\\": "test/"
        },
        "classmap": ["data/gen-php/"]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Atsushi Takayama",
            "email": "taka.atsushi@gmail.com"
        }
    ]
}
